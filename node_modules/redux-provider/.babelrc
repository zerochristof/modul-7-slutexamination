{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-flow",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "shippedProposals": true
      }
    ]
  ],
  "plugins": [
    "lodash",
    "@babel/proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-transform-runtime", {
      "corejs": false,
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }]
  ]
}
